<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main_graph"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="keijumt.navigationsample.home.HomeFragment"
        android:label="homeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/search"
        android:name="keijumt.navigationsample.search.SearchFragment"
        android:label="searchFragment"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/notification"
        android:name="keijumt.navigationsample.noti.NotificationFragment"
        android:label="notificationFragment"
        tools:layout="@layout/fragment_notification" />

    <dialog
        android:id="@+id/alert_dialog"
        android:name="keijumt.navigationsample.dialog.AlertDialogFragment"
        android:label="AlertDialogFragment" />

</navigation>