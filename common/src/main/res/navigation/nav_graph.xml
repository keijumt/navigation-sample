<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragment_launcher">

    <fragment
        android:id="@+id/fragment_launcher"
        android:name="keijumt.navigationsample.launcher.LauncherFragment"
        android:label="launcherFragment"
        tools:layout="@layout/fragment_launcher">
        <action
            android:id="@+id/action_fragment_launcher_to_fragment_main"
            app:destination="@id/fragment_main" />
    </fragment>

    <fragment
        android:id="@+id/fragment_main"
        android:name="keijumt.navigationsample.main.MainFragment"
        android:label="mainFragment"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/fragment_home"
        android:name="keijumt.navigationsample.home.HomeFragment"
        android:label="homeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/fragment_search"
        android:name="keijumt.navigationsample.search.searchFragment"
        android:label="searchFragment"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/fragment_notification"
        android:name="keijumt.navigationsample.notification.notificationFragment"
        android:label="notificationFragment"
        tools:layout="@layout/fragment_notification" />
</navigation>